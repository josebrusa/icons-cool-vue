{
  "name": "icons-cool-vue",
  "version": "0.1.0",
  "description": "Biblioteca de iconos como componentes Vue (Vue 3 + TypeScript)",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
