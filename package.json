    "url": "https://github.com/josebrusa/icons-cool-vue.git"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.30.0",
    "svgo": "^3.3.2",
    "typescript": "^5.4.0",
    "vite": "^7.0.0",
    "vitest": "^3.2.0",
    "vue": "^3.4.0"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  }
